#!/usr/bin/env node

const program = require('commander');
const package = require('../package.json');

process.title = 'qing';

program
  .version(package.version, '-v, --version')

program
  .command('build [page]')
  .alias('b')
  .description('build your page, build [page]')
  .action((page) => {
    console.log('build' + page);
  });
program.parse(process.argv);